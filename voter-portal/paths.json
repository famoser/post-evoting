	"src": "./src/less",
	"entryFile": "soocss.less",
	"dist": "./dist/css",
	"bundleFile": "voting-portal.css"
  },
  "css": {
	"src": "./src/css",
	"dist": "./dist/css"
  },
  "assets": {
	"src": "./src/assets",
	"dist": "./dist/assets"
  },
  "views": {

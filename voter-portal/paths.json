{
  "global": {
	"node_modules": "./node_modules",
	"src": "./src",
	"dist": "./dist",
	"test": "./test",
	"vendors": "./src/vendors",
	"common": "./src/common",
	  "ovApi": "./ov-api-lib"
  },
  "js": {
	"src": "./src/app",
	"entryFile": "./src/app/app.js",
	"vendorsEntryFile": "./src/app/vendors.js",
	"dist": "./dist",
	"bundleFile": "voting-portal.js",
	"vendorsBundleFile": "voting-portal-vendors.js"
  },

	"src": "./src/app",
	"dist": "./dist/",
	"bundleFile": "voting-portal-templates.js",
	"moduleName": "app.template",
	"prefix": ""
  },
  "index": {
	"src": "./src/index.html",
	"dist": "./dist/index.html"
  },
  "i18n": {
	"src": "./src/i18n",
	"dist": "./dist/assets/lang"
  },
  "unitTests": {
	"baseDir": "./tests/unit",
	"angularMocks": "./node_modules/angular-mocks/angular-mocks.js"
  },
  "e2eTests": {
	"baseDir": "./tests/e2e"

{
  "esversion": 6,
  "bitwise": false,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "funcscope": true,
  "futurehostile": true,
  "globals": {
    "forge": true,
    "module": true,
    "exports": true,
    "require": true,
    "process": true
  },
